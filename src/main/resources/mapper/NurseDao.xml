<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.NurseDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.NurseEntity" id="nurseMap">
        <result property="nurseaccountnumber" column="nurseaccountnumber"/>
        <result property="password" column="password"/>
        <result property="nursename" column="nursename"/>
        <result property="avatar" column="avatar"/>
        <result property="gender" column="gender"/>
        <result property="age" column="age"/>
        <result property="mobilenumber" column="mobilenumber"/>
        <result property="clicktime" column="clicktime"/>
        <result property="clicknum" column="clicknum"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.NurseVO" >
		SELECT * FROM nurse  nurse         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.NurseVO" >
		SELECT  nurse.* FROM nurse  nurse
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.NurseView" >

		SELECT  nurse.* FROM nurse  nurse 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.NurseView" >
		SELECT * FROM nurse  nurse <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

	
</mapper>
